{
  "name": "rating-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint:ts": "next lint",
    "lint:css": "stylelint src/**/*.css",
    "lint:ts:fix": "next lint --fix",
    "lint:css:fix": "stylelint src/**/*.css --fix",
    "lint": "pnpm run lint:ts && pnpm run lint:css",
    "lint:fix": "pnpm run lint:ts:fix && pnpm run lint:css:fix",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "gen": "plop component",
    "gen:page": "plop page",
    "gen:lib": "plop lib",
    "prepare": "husky"
  },
  "dependencies": {
    "next": "14.2.16",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@feature-sliced/eslint-config": "^0.1.1",
    "@storybook/addon-a11y": "^8.4.1",
    "@storybook/addon-essentials": "^8.4.1",
    "@storybook/addon-interactions": "^8.4.1",
    "@storybook/addon-links": "^8.4.1",
    "@storybook/blocks": "^8.4.1",
    "@storybook/nextjs": "^8.4.1",
    "@storybook/react": "^8.4.1",
    "@storybook/test": "^8.4.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.16",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-boundaries": "^4.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-storybook": "^0.10.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "lint-staged": "^15.2.10",
    "msw": "^2.6.0",
    "msw-storybook-addon": "^2.0.3",
    "plop": "^4.0.1",
    "prettier": "^3.3.3",
    "storybook": "^8.4.1",
    "stylelint": "^16.10.0",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "src/**/*.css": "stylelint --fix"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
